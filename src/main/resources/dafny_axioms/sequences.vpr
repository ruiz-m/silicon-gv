        forall s: $Seq[E], n: Int, j: Int :: { Seq_index(Seq_take(s, n), j) } { Seq_index(s, j), Seq_take(s, n) }
            0 <= j && j < n && j < Seq_length(s) ==>
                Seq_index(Seq_take(s, n), j) == Seq_index(s, j)
    }

