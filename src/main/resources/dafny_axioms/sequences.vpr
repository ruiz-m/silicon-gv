        forall s: $Seq[E], n: Int :: { Seq_length(Seq_take(s, n)) } { Seq_take(s, n), Seq_length(s) }
            (0 <= n ==>
                (n <= Seq_length(s) ==> Seq_length(Seq_take(s, n)) == n) &&
                (Seq_length(s) < n ==> Seq_length(Seq_take(s, n)) == Seq_length(s))
